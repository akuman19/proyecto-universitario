{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/proyecto-universitario/propiedades.html",
      "destination": "/proyecto-universitario/propiedades.html"
    },
    {
      "source": "/proyecto-universitario/index.html",
      "destination": "/proyecto-universitario/index.html"
    },
    {
      "source": "/proyecto-universitario/",
      "destination": "/proyecto-universitario/index.html"
    },
    {
      "source": "/proyecto-universitario",
      "destination": "/proyecto-universitario/index.html"
    },
    {
      "source": "/propiedades.html",
      "destination": "/proyecto-universitario/propiedades.html"
    },
    {
      "source": "/propiedades",
      "destination": "/proyecto-universitario/propiedades.html"
    },
    {
      "source": "/",
      "destination": "/proyecto-universitario/index.html"
    }
  ],
  "headers": [
    {
      "source": "/proyecto-universitario/assets/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/proyecto-universitario/assets/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/proyecto-universitario/img/(.*\\.(jpg|jpeg|png|gif|webp))",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/proyecto-universitario/img/(.*\\.jpeg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

